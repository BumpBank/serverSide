{
  "name": "bumpbank",
  "version": "1.0.0",
  "description": "it's a real sweet app",
  "main": "index.js",
  "scripts": {
    "client": "budo . -d --serve public/bundle.js --live",
    "start": "nodemon ./bin/www",
    "build": "watchify . -o public/bundle.js -g uglifyify",
    "test": "browserify test/index.js | node | tap-spec",
    "concurrently": "concurrently --kill-others \"npm run start\" \"npm run client\""
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mel-booth zimsteve/bumpbank"
  },
  "author": "mel-booth zimsteve",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mel-booth zimsteve/bumpbank/issues"
  },
  "homepage": "https://github.com/mel-booth zimsteve/bumpbank",
  "dependencies": {
    "clone": "^2.1.0",
    "redux": "^3.6.0",
    "superagent": "^3.1.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "budo": "^8.2.0",
    "gh-pages": "^0.11.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1",
    "uglifyify": "^3.0.1",
    "watchify": "^3.7.0",
    "bcrypt": "^0.8.7",
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "express": "~4.13.1",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.14.2",
    "knex": "^0.12.6",
    "morgan": "^1.7.0",
    "nodemon": "^1.11.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "pg": "^6.1.0",
    "sqlite3": "^3.1.8",
    "supertest": "^2.0.1",
    "concurrently": "^3.1.0",
    "cors": "^2.8.1"
  }
